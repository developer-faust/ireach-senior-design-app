<?xml version="1.0" encoding="utf-8" ?>
<nutritionInfo:FoodDetailPageXaml xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IReach.Pages.Food.User.FoodDetailPage"
             xmlns:nutritionInfo="clr-namespace:IReach.Pages.Food.User;assembly=IReach">

  <nutritionInfo:FoodDetailPage.Content>
    <StackLayout x:Name="content">
      <Label Text="Food Name Goes Here"/>
      <StackLayout Orientation="Horizontal" HorizontalOptions="CenterAndExpand">
        <Label Text="Calories: "/>
        <Label Text="Calories Amount Here"/>
      </StackLayout>
      <StackLayout Orientation="Horizontal" HorizontalOptions="CenterAndExpand">
        <Label Text="Calcium: "/>
        <Label Text="Calcium Amount Here"/>
      </StackLayout>
      <StackLayout Orientation="Horizontal" HorizontalOptions="CenterAndExpand">
        <Label Text="Iron: "/>
        <Label Text="Iron Amount Here"/>
      </StackLayout>
      <StackLayout Orientation="Horizontal" HorizontalOptions="CenterAndExpand">
        <Label Text="Magnesium: "/>
        <Label Text="Magnesium Amount Here"/>
      </StackLayout>
      <StackLayout Orientation="Horizontal" HorizontalOptions="CenterAndExpand">
        <Label Text="Potassium: "/>
        <Label Text="Potassium Amount Here"/>
      </StackLayout> 
    </StackLayout> 
    
  </nutritionInfo:FoodDetailPage.Content>

</nutritionInfo:FoodDetailPageXaml>